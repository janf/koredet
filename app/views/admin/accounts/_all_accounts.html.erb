<h3>Accounts</h3>
<table  class="table table-striped", style="width: auto; height: auto">
  	<thead>
	    <tr>
		    <th>Account name</th>
		    <th>Create</th>
		    <th>Administrators</th>
		    <th>Id</th>
		    <th>Action</th>
	    </tr>
	</thead>

  	<tbody>
		<% @accounts.each do |a| %>
	      	<tr>
			  	<td> <%= a.account_name %></td>
			  	<td> <%= a.created_at %></td>
			  	<td>
			  	<% a.account_admins.each do |adm| %>
	      		 	<%= adm.user.email %>;
	      		<% end %>
	      		</td>
	      		<td><%= a.id.to_s %> </td>
	      		<td>
	      		<%= link_to 'Show details', admin_account_path(a)  %>
	      		</td>
			<tr>
		<% end %>	
	 </tbody>
</table>