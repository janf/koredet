<p id="notice"><%= notice %></p>

<h1>Apiv1 Locations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apiv1_locations.each do |apiv1_location| %>
      <tr>
        <td><%= link_to 'Show', apiv1_location %></td>
        <td><%= link_to 'Edit', edit_apiv1_location_path(apiv1_location) %></td>
        <td><%= link_to 'Destroy', apiv1_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Apiv1 Location', new_apiv1_location_path %>
